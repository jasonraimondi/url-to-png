{
  "name": "@jmondi/url-to-png",
  "version": "1.3.1",
  "description": "URL-to-PNG Server",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist/*",
    "serve": "node -r dotenv/config dist/main.js",
    "dev": "tsx --watch src/main.ts | pino-pretty",
    "compile": "tsc --project tsconfig.build.json",
    "build": "run-s clean compile",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "engines": {
    "node": ">= 20",
    "pnpm": ">= 8.4.0"
  },
  "engineStrict": true,
  "dependencies": {
    "@aws-sdk/client-s3": "^3.537.0",
    "@hono/node-server": "^1.8.2",
    "@jmondi/string-encrypt-decrypt": "^0.0.6",
    "dotenv": "^16.4.5",
    "generic-pool": "^3.9.0",
    "hono": "^4.1.3",
    "md5": "^2.3.0",
    "nano": "^10.1.3",
    "pino": "^8.19.0",
    "playwright": "^1.42.1",
    "sharp": "^0.33.2",
    "zod": "^3.22.4",
    "zod-ff": "^1.4.0"
  },
  "devDependencies": {
    "@types/md5": "^2.3.5",
    "@types/node": "^20.11.30",
    "@vitest/coverage-v8": "^1.4.0",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "vitepress": "1.0.0-rc.45",
    "vitest": "^1.4.0",
    "vue": "3.4.21"
  }
}
